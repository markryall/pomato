{
  "name": "pomato",
  "version": "0.0.1",
  "description": "pomodoro react application",
  "repository": {
    "type": "git",
    "url": "git@github.com:markryall/pomato.git"
  },
  "scripts": {
    "build": "npm run build-scripts",
    "build-dev": "npm run build-scripts-dev",
    "build-scripts": "node_modules/.bin/webpack -p",
    "build-scripts-dev": "node_modules/.bin/webpack",
    "watch": "node_modules/.bin/webpack --watch",
    "up": "node_modules/.bin/forever start server.js",
    "down": "node_modules/.bin/forever stopall"
  },
  "dependencies": {
    "compression": "^1.2.2",
    "express": "^4.10.6",
    "forever": "^0.13.0",
    "morgan": "^1.5.0"
  },
  "devDependencies": {
    "jsx-loader": "^0.12.2",
    "lodash": "^2.4.1",
    "oro-dispatcher": "^1.0.1",
    "react": "^0.12.1",
    "webpack": "^1.4.15"
  }
}
